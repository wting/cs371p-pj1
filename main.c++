int eval() {
	int x = i, y = j;
	if (x > y)
		swap (y,x);

	int max = 0, tmp = 0;
	for (int n=x; n<=y; ++n) {
          if (n % 100 == 1 && y - n >= 100) {
            tmp = meta[n/100];
            n += 100;
          } else {
		tmp = eval_iterative(0,n);
          }

		if (tmp > max)
			max = tmp;
	}

	v = max;
	return max;
}

int eval_iterative(int cycle, unsigned long int n) {
	int cycle_path[1000] = {0};
	int path_loc = 0;

	while (n != 1) {
		if (n < 1000000 && lazy[n] != 0) {
			cycle += lazy[n] - 1;
			break;
		}

		cycle_path[path_loc++] = n;
		if (n%2 == 1) {
			n = 3 * n + 1;
			cycle_path[path_loc] = n;
		}
		else
			n /= 2;
		++cycle;
	}
	++cycle;

	path_loc = 0;
	int c = 0;
	while (path_loc <= 1000) {
		c = cycle_path[path_loc++];
		if (c == 0 || c > 1000000)
			break;
		int cycle_count = cycle - (path_loc - 1);
		lazy[c] = cycle_count;
	}

	return cycle;
}

int eval_recursive(int cycle, unsigned long int n) {
	if (n == 1)
		return cycle;
	if (n%2 == 1)
		return eval_recursive(cycle+2,n + (n>>1) + 1);
	else
		return eval_recursive(cycle+1,n>>1);
}


